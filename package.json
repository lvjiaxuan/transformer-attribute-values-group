{
  "private": false,
  "name": "unocss-transformer-attribute-values-group",
  "type": "module",
  "version": "0.2.2",
  "description": "Attribute values group transformer for UnoCSS.",
  "keywords": [
    "unocss",
    "unocss-transformer"
  ],
  "scripts": {
    "test": "vitest",
    "lint": "eslint .",
    "release": "lvr --no-push",
    "build:cjs": "rollup index.mjs --file=index.cjs --format=cjs --external=magic-string",
    "prepublishOnly": "pnpm test && pnpm build:cjs"
  },
  "main": "index.cjs",
  "module": "index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs",
      "types": "./index.d.ts"
    }
  },
  "author": "lvjiaxuan <471501748@qq.com> (https://github.com/lvjiaxuan)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lvjiaxuan/unocss-transformer-attribute-values-group"
  },
  "devDependencies": {
    "@lvjiaxuan/eslint-plugin": "^0.16.0",
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "rollup": "^3.28.0",
    "vitest": "^0.34.1"
  },
  "dependencies": {
    "@unocss/core": "^0.55.0",
    "magic-string": "^0.30.2"
  }
}